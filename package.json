{
  "name": "netlify-lambda-functions",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently 'npm run start:client' 'npm run start:server'",
    "start:client": "webpack-dev-server --mode development --hot --config webpack.client.js",
    "start:server": "netlify-lambda serve src/lambda -c webpack.server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --mode production --config webpack.client.js",
    "build:server": "netlify-lambda build src/lambda",
    "build:lambda": "netlify-lambda build src/lambda"
  },
  "keywords": [
    "javascript",
    "netlify",
    "lambda"
  ],
  "author": "Cristian Moreno <khriztianmoreno@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "netlify-lambda": "^1.6.3",
    "node-emoji": "^1.10.0",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "babel-loader": "8.1.0",
    "concurrently": "5.1.0",
    "dotenv": "8.2.0",
    "extract-loader": "5.0.1",
    "file-loader": "6.0.0",
    "html-loader": "1.0.0",
    "html-webpack-plugin": "4.0.1",
    "markdown-magic": "1.0.0",
    "node-fetch": "2.6.0",
    "webpack": "4.42.1",
    "webpack-dev-server": "3.10.3",
    "webpack-cli": "3.3.11"
  }
}
